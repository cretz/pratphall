Elseif test
SET-OPTION-TRUE:useElseif
-----
var a, b, c: number;
if (1 == 2) a = 5;
else if (2 == 3) a = 6;
if (true) {
    b = 7;
} else if (false) b = 8;
if (false) c = 9;
else if (true) {
    c = 10;
}
-----
if (1 == 2) $a = 5;
elseif (2 == 3) $a = 6;
if (true) {
    $b = 7;
} elseif (false) $b = 8;
if (false) $c = 9;
elseif (true) {
    $c = 10;
}