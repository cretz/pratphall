Pct.PhpAssocArray
-----
var c = Pct.newAssocArray({a: 15, b: 15 });
c[12] = 15;
c['blah'] = 15;
var d = Pct.toArray(c);
var e = Pct.toAssocArray(d);
Pct.newAssocArray({a: 15, b: 15 }).forEach((value: number, index: string) {
	var f = value + ' ' + index;
});
var g = Pct.newAssocArray({
    a: 15,
	b: 15
});
-----
$c = ['a' => 15, 'b' => 15];
$c[12] = 15;
$c['blah'] = 15;
$d = $c;
$e = $d;
foreach (['a' => 15, 'b' => 15] as $index => $value) {
    $f = $value . ' ' . $index;
}
$g = [
    'a' => 15,
    'b' => 15
];