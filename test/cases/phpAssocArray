Pct.PhpAssocArray
-----
var a = <Pct.PhpAssocArray>{
    a: 5, b: 5
};
var b = <Pct.PhpAssocArray>{ a: 10, b: 10 };
var c = Pct.newAssocArray({a: 15, b: 15 });
var d = Pct.toArray(c);
var e = Pct.toAssocArray(d);
Pct.newAssocArray({a: 15, b: 15 }).forEach((value: number, index: string) {
	var f = value + ' ' + index;
});
-----
$a = [
    'a' => 5,
    'b' => 5
];
$b = ['a' => 10, 'b' => 10];
$c = ['a' => 15, 'b' => 15];
$d = $c;
$e = $d;
foreach (['a' => 15, 'b' => 15] as $index => $value) {
    $f = $value . ' ' . $index;
}