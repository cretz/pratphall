try {
    $a = 1;
} catch (Exception $e) { }
try {
    $a = 1;
} finally { }
try {
    $a = 1;
} catch (Exception $e) {
    $b = 1;
} finally {
    $c = 1;
}
try {
    $a = 1;
} finally {
    $b = 1;
}
try {
    $a = 1;
} catch (Exception $e) {
    $b = 1;
} finally {
    $c = 1;
}
try {
    $a = 1;
} catch (RuntimeException $e) {
    $b = $e->getMessage();
} catch (Exception $e) {
    $c = 1;
}
try {
    $a = 1;
} catch (Exception $_tmp1) { }