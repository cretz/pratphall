Try test
-----
try {
    var a = 1;
} catch (e) { }
try {
    var a = 1;
} finally { }
try {
    var a = 1;
} catch (e) {
    var b = 1;
} finally {
    var c = 1;
}
try {
    var a = 1;
} finally {
    var b = 1;
}
Pct.try({
    try: () => {
        var a = 1;
    },
    catch: (e: Exception) => {
        var b = 1;
    },
    finally: () => {
        var c = 1;
    }
});
Pct.try({
    try: () => {
        var a = 1;
    },
    catch: [
        (e: RuntimeException) => {
            var b = e.getMessage();
        },
        (e: Exception) => {
            var c = 1;
        }
    ]
});
Pct.try({
    try: () => {
        var a = 1;
    }
});
